{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "google": false
  },
  "rules": {
   "indent": [2, 2],
   "quotes": [2, "single"],
   "no-console": 0,
   "no-use-before-define": [2, "nofunc"],
   "no-underscore-dangle": 0,
   "no-constant-condition": 0,
   "space-before-function-paren":[2, {"anonymous": "always", "named": "never"}],
   // "func-style": [2, "declaration"],
   "semi": 2
  },
  "parser": "babel-eslint"
}
