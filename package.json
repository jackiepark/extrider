{
  "name": "extrider",
  "version": "1.8.0",
  "bin": {
    "strider": "bin/strider"
  },
  "main": "main.js",
  "description": "Brilliant continuous deployment platform",
  "license": "BSD-3-Clause",
  "keywords": [
    "extrider",
    "strider",
    "continuous integration",
    "continuous deployment",
    "testing",
    "deployment"
  ],
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Pitzcarraldo/extrider.git"
  },
  "scripts": {
    "test": "make test",
    "lint": "eslint client lib --fix",
    "start": "bin/strider",
    "dev": "rimraf dist && nodemon --ignore client/ bin/strider",
    "build": "rimraf dist && webpack --verbose --colors --display-error-details --config webpack/build.config.js",
    "unitcov": "istanbul cover _mocha --recursive test/unit",
    "unitdev": "mocha -w --recursive test/unit -R min",
    "gendocs": "apidoc -i lib/ -o apidocs/",
    "debug": "node debug bin/strider --no-cluster"
  },
